<link rel="stylesheet" href="/css/rutina/rutina.css">
<form action="/rutina/Agregar/{{user.idUsuarios}}" method="post">

    <div id="contenido" class="row">
        <div id="titulo">
            <p>Rutinas</p>
        </div>
        <label>video_rutina: </label>
        <div id="video_rutina">
            <input name="video_rutina" id="video_rutinas" checked type="text">
        </div>

        <label>tiempo_rutina: </label>
        <div id="tiempo_rutina">
            <input name="tiempo_rutina" id="tiempo_rutinas" checked type="text">
        </div>

        <label>descripcion: </label>
        <div id="descripcion">
            <input name="descripcion" id="descripciones" checked type="text">
        </div>

        <label>progreso: </label>
        <div id="progreso">
            <input name="progreso" id="progresos" checked type="text">
        </div>
        
        <div id="agregar">
            <button type="submit">Agregar</button>
        </div>
    </div>
</form>
