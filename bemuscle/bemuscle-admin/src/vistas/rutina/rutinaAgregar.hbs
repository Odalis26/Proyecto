<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<link rel="stylesheet" href="/css/rutina/agregar.css">
<br>
<section class="container">
    <form action="/rutina/Agregar" method="post">

        <div id="contenido" class="row g-3">
            <div id="titulo">
                <p>Rutinas</p>
            </div>
           
          
               <div class="row">
                    <div id="video_rutina" class="col-auto">
                 <label>video_rutina: </label>
                <input name="video_rutina" class="form-control" id="video_rutinas" checked type="text">
            </div>

            
            <div id="tiempo_rutina" class="col-auto">
                <label>tiempo_rutina: </label>
                <input name="tiempo_rutina" class="form-control" id="tiempo_rutinas" checked type="text">
            </div>
               </div>

           <div class="row">
                <label>descripcion: </label>
            <div id="descripcion" class="col-auto">
                <input name="descripcion" class="form-control" id="descripciones" checked type="text">
            </div>

            <label>progreso: </label>
            <div id="progreso" class="col-auto">
                <input name="progreso" class="form-control" id="progresos" checked type="text">
            </div>
           </div>




            <div id="agregar" class="col-auto">
                <button type="submit" class="btn btn-primary mb-3">Agregar</button>
            </div>
           </div>
        </div>
    </form>

</section>