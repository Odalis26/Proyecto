<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<link rel="stylesheet" href="/css/rutina/rutina.css">
{{#each lista}}
<form action="/rutina/Editar/{{rutina_id}}" method="post">
    {{/each}}
    <div id="contenido" class="row g-3">
        <div id="titulo">
            <p>Rutina</p>
        </div>
        {{#each lista}}
        <label>video_rutina: </label>
        <div id="video_rutina" class="col-auto">
            <input name="video_rutina" class="form-control" id="video_rutinas" checked
                type="text">
        </div>

        <label>tiempo_rutina: </label>
        <div id="tiempo_rutina" class="col-auto">
            <input name="tiempo_rutina" class="form-control" id="tiempo_rutinas" checked
                type="text">
        </div>

         <label>descripcion: </label>
        <div id="descripcion" class="col-auto">
            <input name="descripcion" class="form-control" id="descripciones" checked
                type="text">
        </div>

           <label>progreso: </label>
        <div id="progreso" class="col-auto">
            <input name="progreso" class="form-control" id="progresos" checked
                type="text">
        </div>
        {{/each}}
        <div id="editar"  class="col-auto">
            <button type="submit" class="btn btn-primary mb-3">Editar</button>
        </div>
    </div>
</form>