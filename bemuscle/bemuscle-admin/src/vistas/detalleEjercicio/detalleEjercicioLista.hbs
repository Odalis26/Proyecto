<link rel="stylesheet" href="/css/detalle_ejercicio/detalleEjercicioLista.css">
<form action="/detalleEjercicio/Lista/{{user.idUsuarios}}" method="get">

   <div id="contenido">

        <div id="fondo">
            <img src="/img/fondoPesa1.png" alt="">
        </div>
        <div id="contenedor">
        <div class="row">
            {{#each lista}}
            <div id="tarjetas">
                <div id="comentario">
                    <p> {{comentarios}}</p>
                </div>
                <div id="eliminar">
                    <a href="/detalleEjercicio/Eliminar/{{detalle_ejercicio_id}}"> Eliminar</a>
                </div>
                <div id="Editar">
                    <a href="/detalleEjercicio/Editar/{{detalle_ejercicio_id}}">Editar</a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>

</form>