<link rel="stylesheet" href="/css/detalleRutina/detalleRutina.css">
<form action="/detalleRutina/Agregar/{{user.idUsuarios}}" method="post">
    <div id="img">
        <img src="/img/ejecutivo_logo.png" alt="">
    </div>
    <div id="contenido">

        <div id="contenedor">

            <div id="titulo">
                <p>Detalle Rutina</p>
            </div>
            <label>Consejos: </label>
            <div id="consejos">
                <input name="consejos" id="consejo" checked type="text">
            </div>
            <div id="agregar">
                <button type="submit">Agregar</button>
            </div>
        </div>

    </div>
</form>